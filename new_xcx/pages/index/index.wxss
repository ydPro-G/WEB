.header {
  width: 750rpx;
  height: 90rpx;
  border: 1rpx solid red;
}
.hdCon {
  display: flex;
  /* 项目两端对齐 */
  justify-content: space-between;
  /* 项目据居中对齐 */
  align-items: center;
  /* 继承父级高度，不然aliggn-items用不了 */
  height: 100%;
}
/* ,重新写 */
.hdCon navigator,.hdCon .kefu{
  height: 50rpx;
}


.hdCon image{
  height: 100%;
}
.kefu {
  position: relative;
    /* 动画，调用app.wxss写的dh样式，linear(匀速)，1s(时间) infinite(无限循环) */
    animation: dh linear 1s infinite alternate;
}
.kefu .btn{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
  background-color: #fff;
}

